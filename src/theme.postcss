	--color-surface-900: 0 28 56; /* ⬅ #001C38 */
	
}

[data-theme='custom'] h1,
[data-theme='custom'] h2,
[data-theme='custom'] h3,
[data-theme='custom'] h4,
[data-theme='custom'] h5,
[data-theme='custom'] h6,
[data-theme='custom'] a,
[data-theme='custom'] button {
	font-weight: bold;
}

[data-theme='custom'] {
	/* prettier-ignore */
	background-image:
        radial-gradient(at 82% 48%, hsla(209,100%,90%,1) 0px, transparent 50%),
        radial-gradient(at 80% 100%, hsla(209,100%,80%,1) 0px, transparent 50%),
        radial-gradient(at 0% 0%, hsla(210,100%,94%,1) 0px, transparent 50%);
	background-attachment: fixed;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
}
.dark [data-theme='custom'] {
	/* prettier-ignore */
	background-image:
		radial-gradient(at 76% 0%, hsla(189,100%,56%,0.20) 0px, transparent 50%),
		radial-gradient(at 1% 0%, hsla(340,100%,76%,0.15) 0px, transparent 50%),
		radial-gradient(at 20% 100%, hsla(241,100%,70%,0.30) 0px, transparent 50%);
	background-attachment: fixed;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
}