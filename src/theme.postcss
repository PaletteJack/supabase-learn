	--color-tertiary-200: 191 242 233; /* ⬅ #bff2e9 */
	--color-tertiary-300: 153 235 220; /* ⬅ #99ebdc */
	--color-tertiary-400: 77 219 193; /* ⬅ #4ddbc1 */
	--color-tertiary-500: 0 204 167; /* ⬅ #00cca7 */
	--color-tertiary-600: 0 184 150; /* ⬅ #00b896 */
	--color-tertiary-700: 0 153 125; /* ⬅ #00997d */
	--color-tertiary-800: 0 122 100; /* ⬅ #007a64 */
	--color-tertiary-900: 0 100 82; /* ⬅ #006452 */
	/* success | #36d129 */
